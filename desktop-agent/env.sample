OPENAI_API_KEY=
SERVER_URL=http://localhost:4000

HOTKEY=ctrl+alt+space
OPENAI_MODEL=gpt-4o-mini
PROMPT=Solve the problem shown in this image. Show your work.

STREAM_HOTKEY=alt+w
STREAM_WHISPER_MODEL=large-v3
STREAM_COMPUTE_TYPE=int8_float16
STREAM_LANGUAGE=zh
STREAM_SAMPLE_RATE=16000
STREAM_CHUNK_SECONDS=3
STREAM_BEAM_SIZE=2
STREAM_GPT_MODEL=qwen-max
STREAM_PROMPT=Answer the interviewer's question clearly and concisely.
STREAM_MIN_SECONDS_BETWEEN_ANSWERS=8
STREAM_SEND_HOTKEY=alt+e
STREAM_MANUAL_ONLY=false
STREAM_SILENCE_THRESHOLD=0.015
STREAM_SILENCE_MS=700
STREAM_MIN_SEGMENT_SECONDS=1.2
STREAM_MAX_SEGMENT_SECONDS=8.0
STREAM_LOOPBACK_DEVICE_NAME=

AUDIO_HOTKEY=alt+q
AUDIO_GPT_MODEL=qwen-max
AUDIO_STT_MODEL=large-v3
AUDIO_PROMPT=Answer the interviewer's question clearly and concisely.

CONTROL_SCROLL_HOTKEY=ctrl+alt+down
CONTROL_SCROLL_DELTA=400
CONTROL_SCROLL_UP_HOTKEY=ctrl+alt+up
